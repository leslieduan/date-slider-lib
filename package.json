{
  "name": "date-slider-lib",
  "version": "0.1.10",
  "description": "A React TypeScript UI component library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./style.css": "./dist/date-slider-lib.css",
    "./dist/date-slider-lib.css": "./dist/date-slider-lib.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*.{js,mjs,cjs,css,d.ts}",
    "!dist/**/*.map"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "pnpm format:check && pnpm type-check",
    "pack-local": "pnpm build && npm pack"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "react",
    "react-component",
    "typescript",
    "date-slider",
    "date-picker",
    "date-range",
    "slider",
    "datepicker",
    "ui-component",
    "component-library",
    "tailwind",
    "tailwindcss"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "homepage": "https://github.com/aodn/imos-live-date-slider#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aodn/imos-live-date-slider.git"
  },
  "bugs": {
    "url": "https://github.com/aodn/imos-live-date-slider/issues"
  },
  "packageManager": "pnpm@10.24.0",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-a11y": "^10.1.2",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-onboarding": "^10.1.2",
    "@storybook/addon-vitest": "^10.1.2",
    "@storybook/react": "^10.1.2",
    "@storybook/react-vite": "^10.1.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^10.1.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "tailwind-merge": "^3.4.0"
  }
}
